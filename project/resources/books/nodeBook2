SELECT	    (REPLACE(str(?book), "http://www.wikidata.org/entity/", "")as ?book)
	#?book 
	?titleLabel ?genreLabel ?released       	
 WHERE
 {
 		#"http://www.wikidata.org/entity/Q7834141"			wd:Q571		?book .
		?book			wdt:P50			?author .
		?author			rdfs:label		?authorLabel .
   		?book			rdfs:label		?titleLabel .
		?book			wdt:P136		?genre .
   		?genre			rdfs:label		?genreLabel .
   		?book			wdt:P577		?publicationDate .
   		
		FILTER(lang(?authorLabel) = "en") .
   		FILTER(lang(?titleLabel) = "en") .
   		FILTER(lang(?genreLabel) = "en") .
   		#FILTER(str(?authorLabel) = "Paulo Coelho") .
   		#FILTER(str(?titleLabel) = "The Alchemist") .
   		
   		BIND(str(year(?publicationDate)) AS ?released)
}
GROUP BY
		?book ?titleLabel ?released ?genreLabel
ORDER BY
		?titleLabel